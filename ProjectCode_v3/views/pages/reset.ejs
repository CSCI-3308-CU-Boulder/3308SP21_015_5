   <body class="center text-center" id="centerbody">
     <button type="button" id="darkMode" onclick="darkMode()" >Dark Mode</button>
 	<div class="loginbox">
 		<img src="../resources/img/profile.jpg" class="profilePic" alt="Placeholder profile picture">
 		<h1 class="loginTitle">Reset your password</h1>
 		<form id="/login/login_" method="get" id="login">
 			<p>Username</p>
 			<input id="username" type="text" name="username" placeholder="Enter Username" >
 			<p>What city were you born in?</p>
 			<input id="security" type="text" name="security" placeholder="Enter City">
             <p>New Password</p>
 			<input id="newpass" type="password" name="newpass" placeholder="Enter new password" >
           <input type="submit" href="/home" value="submit"  class="btn btn-primary btn-block">
			

			
 		</form>

 	</div>
